<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ipsensor"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="IpSensor"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.IpSensor.MainActivity"
            android:label="IpSensor" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.IpSensor.DisplayMessage"
            android:label="@string/title_activity_display_message"
            android:parentActivityName="com.example.traceroute.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.traceroute.MainActivity" />
        </activity>
        <activity
            android:name="com.example.IpSensor.NextActivity"
            android:label="@string/title_activity_next"
            android:parentActivityName="com.example.traceroute.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.traceroute.MainActivity" />
        </activity>
        <activity
            android:name="com.example.IpSensor.Traceroute"
            android:label="@string/title_activity_traceroute" >
        </activity>
        <activity
            android:name="com.example.IpSensor.ScanActivity"
            android:label="@string/title_activity_scan" >
        </activity>
        <activity
            android:name="com.example.IpSensor.NextActivityScan"
            android:label="@string/title_activity_next_activity_scan" >
        </activity>
        <activity
            android:name="com.example.IpSensor.OutputScan"
            android:label="@string/title_activity_output_scan" >
        </activity>
    </application>

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    

</manifest>
